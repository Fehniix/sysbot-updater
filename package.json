  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:Fehniix/sysbot-updater.git"
  },
  "keywords": [
    "api",
    "acnh",
    "ACNHOrders",
    "bot",
    "acnh-bot",
    "communication",
    "socket",
    "api"
  ],
  "author": "Johnny Bueti",
  "license": "MIT",
  "bin": {
    "sysbot-update": "./dist/index.js"
  },
  "bugs": {
    "url": "https://github.com:Fehniix/sysbot-updater.git/issues"
  },
  "homepage": "https://github.com:Fehniix/sysbot-updater.git#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.21"
  },
  "dependencies": {
    "fs-extra": "^10.0.1",
    "meow": "^10.1.2"
  }
}
