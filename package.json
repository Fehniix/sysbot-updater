{
  "name": "sysbot-updater",
  "version": "0.1.0",
  "description": "A very lightweight package that automates the process of updating your SysBot-ACNHOrders installation.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prepublish": "npm run build",
    "start": "npm run build && ./dist/index.js",
